{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Basic React SSR setup with Express",
  "main": "index.js",
  "scripts": {
    "start": "npm run server-side-react & npm run sass-watch",
    "server-side-react": "nodemon server.js --exec babel-node --presets @babel/env,@babel/react",
    "sass-watch": "sass --style=compressed --watch ./scss/index.scss ./public/css/styles.min.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reyarqueza/react-ssr.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/reyarqueza/react-ssr/issues"
  },
  "homepage": "https://github.com/reyarqueza/react-ssr#readme",
  "dependencies": {
    "@babel/core": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "babel-loader": "^8.2.2",
    "express": "^4.17.1",
    "nodemon": "^2.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.32.8",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0"
  }
}
